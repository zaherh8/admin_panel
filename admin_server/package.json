{
  "name": "postlight-bo-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server-dev": "NODE_ENV=development nodemon ./bin/www -e ejs,js,css,html,jpg,png,scss",
    "server-prod": "NODE_ENV=production nodemon ./bin/www -e ejs,js,css,html,jpg,png,scss",
    "client": "cd ../admin_client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server-dev\" \"npm run client\"",
    "prod": "\"npm run server-dev\" \"npm run client\""
  },
  "dependencies": {
    "@admin-bro/express": "^3.1.0",
    "admin-bro": "^3.4.0",
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-formidable": "^1.2.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "sequelize-cli": "^6.2.0"
  }
}
